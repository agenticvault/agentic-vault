{
  "mcpServers": {
    "agentic-vault": {
      "command": "npx",
      "args": [
        "-y", "-p", "@agenticvault/agentic-vault",
        "agentic-vault-mcp",
        "--key-id", "YOUR_KMS_KEY_ID",
        "--region", "us-east-1"
      ],
      "env": {
        "AWS_ACCESS_KEY_ID": "${AWS_ACCESS_KEY_ID}",
        "AWS_SECRET_ACCESS_KEY": "${AWS_SECRET_ACCESS_KEY}",
        "AWS_SESSION_TOKEN": "${AWS_SESSION_TOKEN}"
      }
    }
  }
}
