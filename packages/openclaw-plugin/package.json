{
  "name": "@agenticvault/agentic-vault-openclaw",
  "version": "0.1.4",
  "description": "OpenClaw plugin for Agentic Vault â€” expose EVM signing as OpenClaw agent tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "agentic-vault-setup": "./dist/install.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./openclaw.plugin.json": "./openclaw.plugin.json"
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "LICENSE"
  ],
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agenticvault/agentic-vault",
    "directory": "packages/openclaw-plugin"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agenticvault/agentic-vault/issues"
  },
  "homepage": "https://github.com/agenticvault/agentic-vault/tree/main/packages/openclaw-plugin#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "agentic-vault",
    "evm",
    "signing",
    "aws-kms",
    "ai-agent"
  ],
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "dependencies": {
    "@agenticvault/agentic-vault": "~0.1.2"
  },
  "peerDependencies": {
    "@agenticvault/agentic-vault": "~0.1.4",
    "openclaw": ">=2026.1.0"
  },
  "devDependencies": {
    "@agenticvault/agentic-vault": "workspace:*",
    "@eslint/js": "^9.39.2",
    "eslint": "^9.39.2",
    "openclaw": "^2026.2.17",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "tsc -b",
    "typecheck": "tsc -p tsconfig.check.json",
    "lint": "eslint src/ test/",
    "lint:fix": "eslint src/ test/ --fix",
    "test:unit": "vitest run test/unit/",
    "test:integration": "vitest run test/integration/",
    "prepack": "pnpm build"
  }
}
